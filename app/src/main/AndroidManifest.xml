<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yb.com.exchangeviewlocation" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".deamo.ListViewHeaderImageActivity"
            android:label="@string/title_activity_list_view_header_image" >
        </activity>
        <activity
            android:name=".deamo.PullToZoomActivity"
            android:label="@string/title_activity_pull_to_zoom" >
        </activity>
        <activity
            android:name=".deamo.PullToZoomActivity2"
            android:label="@string/title_activity_pull_to_zoom_activity2" >
        </activity>
    </application>

</manifest>
